# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_APP_NAME=Notes App
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=A modern note-taking application

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
REACT_APP_ENABLE_PWA=true

# Authentication Configuration
REACT_APP_TOKEN_STORAGE_KEY=notes_auth_token
REACT_APP_SESSION_TIMEOUT=3600000

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_DEFAULT_LANGUAGE=en

# Performance Configuration
REACT_APP_DEBOUNCE_DELAY=300
REACT_APP_PAGINATION_SIZE=20
REACT_APP_VIRTUAL_LIST_ITEM_HEIGHT=200

# Development Configuration (only used in development)
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_API=false
REACT_APP_LOG_LEVEL=warn

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=10000

# Testing Configuration
REACT_APP_TEST_TIMEOUT=5000
CI=false